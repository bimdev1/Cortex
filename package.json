{
  "name": "cortex-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install",
    "build": "yarn workspaces foreach run build",
    "dev": "docker-compose up -d",
    "dev:nocobase": "ts-node-dev --respawn --transpile-only packages/core/app/server/src/index.ts",
    "dev:workers": "nodemon --exec ts-node packages/workers/jobPoller/index.ts",
    "docs:dev": "cd docs-site && yarn start",
    "docs:build": "cd docs-site && yarn build",
    "docs:serve": "cd docs-site && yarn serve",
    "demo:seed": "ts-node scripts/demo/seed-demo-data.ts",
    "demo:screenshots": "ts-node scripts/demo/generate-screenshots.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:plugins": "yarn workspaces foreach --include '@cortex/*' run test",
    "test:ci": "yarn test && yarn test:plugins"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.5.0",
    "nodemon": "^3.0.0",
    "playwright": "^1.40.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@docusaurus/theme-live-codeblock": "^3.0.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.0",
    "chart.js": "^4.5.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "socket.io-client": "^4.8.1"
  }
}
