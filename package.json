{
  "name": "cortex-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install",
    "build": "yarn workspaces foreach run build",
    "dev": "docker compose up -d",
    "dev:nocobase": "ts-node-dev --respawn --transpile-only packages/core/app/server/src/index.ts",
    "dev:workers": "nodemon --exec ts-node packages/workers/jobPoller/index.ts",
    "docs:api": "typedoc --out docs-site/static/api --entryPointStrategy packages 'packages/providers' 'packages/plugins/@cortex/plugin-core' 'packages/workers' --exclude '**/__tests__/**' --exclude '**/node_modules/**' --exclude '**/*.test.ts' --exclude '**/*.spec.ts'",
    "docs:build": "cd docs-site && yarn build",
    "docs:build-full": "yarn docs:api && cd docs-site && yarn build",
    "docs:dev": "cd docs-site && yarn start",
    "docs:serve": "cd docs-site && yarn serve",
    "demo:seed": "ts-node --project tsconfig.scripts.json scripts/demo/seed-demo-data.ts",
    "demo:screenshots": "ts-node --project tsconfig.scripts.json scripts/demo/generate-screenshots.ts",
    "finalize": "node scripts/finalize-repo.js",
    "polish": "yarn format && yarn lint:fix && yarn test:coverage && yarn docs:build-full && yarn finalize",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.cjs",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:plugins": "yarn workspaces foreach --include '@cortex/*' run test",
    "test:ci": "yarn lint && yarn type-check && yarn test:coverage",
    "prune": "ts-prune",
    "analyze:dead-code": "ts-node --project tsconfig.scripts.json scripts/analyze-dead-code.ts",
    "docs:placeholders": "ts-node --project tsconfig.scripts.json scripts/generate-docs-placeholders.ts",
    "analyze:public-surface": "ts-node --project tsconfig.scripts.json scripts/analyze-public-surface.ts",
    "enforce:boundaries": "ts-node --project tsconfig.scripts.json scripts/enforce-boundaries.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "nodemon": "^3.0.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@docusaurus/theme-live-codeblock": "^3.0.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.0",
    "@nocobase/database": "^1.8.3",
    "@nocobase/server": "^1.8.3",
    "chart.js": "^4.5.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "socket.io-client": "^4.8.1"
  }
}
